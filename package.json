{
  "name": "lens-suite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clear_babel_cache": "rm -rf ./node_modules/.cache/babel-loader",
    "dev": "yarn clear_babel_cache && yarn next dev",
    "build": "yarn clear_babel_cache && yarn next build",
    "start": "yarn clear_babel_cache && yarn next start",
    "lint": "next lint",
    "check_types": "tsc --noEmit",
    "gql:generate": "yarn graphql-codegen --config './graphql-codegen.config.js'"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@rainbow-me/rainbowkit": "^0.4.5",
    "color": "^4.2.3",
    "ethers": "^5.6.9",
    "lodash": "^4.17.21",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5",
    "wagmi": "^0.6.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@graphql-codegen/cli": "^2.11.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@types/color": "^3.0.3",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "18.6.5",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-import-graphql": "^2.8.1",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "graphql": "^16.5.0",
    "typescript": "4.7.4"
  }
}
